  },
  "devDependencies": {
    "eslint": "^0.8.2",
    "font-awesome": "^4.2.0",
    "istanbul": "^0.3.2",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.6.2",
    "lodash": "^2.4.1",
    "tap-spec": "^1.0.0",
    "tape": "^3.0.0"
