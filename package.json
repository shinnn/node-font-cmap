{
  "name": "font-cmap",
  "version": "1.0.1",
  "description": "Parse CMap of a TrueType/OpenType font file buffer",
  "repository": "shinnn/node-font-cmap",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint *.js test/test.js & jscs test/test.js",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/node-font-cmap/blob/master/LICENSE"
    }
  ],
  "files": [
    "cli.js",
    "index.js"
  ],
  "bin": "cli.js",
  "keywords": [
    "buffer",
    "cmap",
    "table",
    "font",
    "glyph",
    "index",
    "id",
    "code-point",
    "json",
    "parse",
    "cli",
    "command-line"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "get-stdin": "^4.0.1",
    "is-buffer": "^1.0.1",
    "minimist": "^1.1.0",
    "opentype.js": "^0.4.4",
    "sum-up": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^0.15.0",
    "font-awesome": "^4.3.0",
    "istanbul": "^0.3.6",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "lodash.isplainobject": "^3.0.0",
    "tap-spec": "^2.2.1",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
